{
  "family": "django-ecs",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "django-ecs",
      "essential": true,
      "image": "973791587081.dkr.ecr.ap-northeast-2.amazonaws.com/django-ecs:latest",
      "cpu": 0,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}